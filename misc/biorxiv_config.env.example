# BioRxiv TDM Download Configuration
# Copy this file to .env and fill in your AWS credentials

# AWS Credentials (required for accessing BioRxiv S3 bucket)
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here

# Optional: AWS Region (defaults to us-east-1)
AWS_DEFAULT_REGION=us-east-1

# MinIO Configuration (optional, defaults to localhost:9002)
MINIO_ENDPOINT=localhost:9002
MINIO_ACCESS_KEY=piotrminio
MINIO_SECRET_KEY=piotrminio

# Download Settings (optional)
MAX_FILES=100
MINIO_BUCKET=biorxiv-papers

